{
  "manifest_version": 2,
  "name": "WideGPT",
  "version": "1.1",
  "description": "Scale GPT output to 100% width",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.chatgpt.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}